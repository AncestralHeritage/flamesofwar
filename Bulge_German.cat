    <selectionEntry id="968c-9f83-36a8-339b" name="Hans-Peter Knaust" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="06f4-1995-6d8b-2b82" type="max"/>
